CREATE VIEW "290_Professional_and_Outside_Services" AS
SELECT SUM("ProfOutServAMT") AS total_amount
FROM (
  SELECT "ProfOutServAMT" FROM "291_Legal_Fees"
  UNION ALL
  SELECT "ProfOutServAMT" FROM "292_Audit_Fees"
  UNION ALL
  SELECT "ProfOutServAMT" FROM "293_Accounting_Services"
  UNION ALL
  SELECT "ProfOutServAMT" FROM "294_Management_Consulting_Fees_"
  UNION ALL
  SELECT "ProfOutServAMT" FROM "299_Other_Professional_and_Outside_Services"
) AS prof_out_serv;

INSERT INTO "291_Legal_Fees" ("ProfOutServAMT", "Date") 
VALUES (1000, '2022-01-01');

INSERT INTO "292_Audit_Fees" ("ProfOutServAMT", "Date") 
