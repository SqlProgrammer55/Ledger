CREATE VIEW "200_Operating_Expenses_(Control)" AS
SELECT SUM("ExpensesAMT") AS total_expenses
FROM (
    SELECT "ExpensesAMT" FROM "210_Compensation"
    UNION ALL
    SELECT "ExpensesAMT" FROM "211_Salaries"
    UNION ALL
    SELECT "ExpensesAMT" FROM "212_Reimbursement_to_Sponsor_for_Services"
    UNION ALL
    SELECT "ExpensesAMT" FROM "219_Other_Compensation"
) AS all_expenses;

-- Insert a new record into 210_Compensation with ExpensesAMT and Date fields
INSERT INTO "210_Compensation" ("ExpensesAMT", "Date") VALUES (1500, '2023-02-20');

-- Update the ExpensesAMT and Date fields for an existing record in 210_Compensation table
UPDATE "210_Compensation" SET "ExpensesAMT" = 2000, "Date" = '2023-02-21' WHERE "id" = 1;

-- Insert a new record into 211_Salaries with ExpensesAMT and Date fields
INSERT INTO "211_Salaries" ("ExpensesAMT", "Date") VALUES (3000, '2023-02-22');

-- Update the ExpensesAMT and Date fields for an existing record in 211_Salaries table
