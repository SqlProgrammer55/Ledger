
CREATE VIEW 120_Investment_Income AS
SELECT SUM(IncomeAMT) AS Total_Investment_Income
FROM (
  SELECT IncomeAMT FROM 121_Income_from_U.S._Government_Obligations_
  UNION ALL
  SELECT IncomeAMT FROM 122_Income_from_Federal_Agency_Securities
  UNION ALL
  SELECT IncomeAMT FROM 123_Income_from_Investment_in_NCUSIF
  UNION ALL
  SELECT IncomeAMT FROM 124_Interest_on_Trading_Securities
  UNION ALL
  SELECT IncomeAMT FROM 125_Income_from_Common_Trust_Investments
  UNION ALL
  SELECT IncomeAMT FROM 126_Income_from_Shares/Deposits/Certificates_of_Corporate_Credit_Unions_
  UNION ALL
  SELECT IncomeAMT FROM 127_Income_from_Shares/Deposits/Certificates_of_Other_Credit_Unions_
  UNION ALL
  SELECT IncomeAMT FROM 128_Income_from_Other_Shares/Deposits/Certificates
  UNION ALL
  SELECT IncomeAMT FROM 128.1_Income_from_Investments_in_Credit_Union_Service_Corporations_(CUSO)_
  UNION ALL
  SELECT IncomeAMT FROM 129_Income_from_Other_Investments
) AS Total_Income;

