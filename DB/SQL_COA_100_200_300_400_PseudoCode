CREATE VIEW 130_Fees_and_Charges AS 
SELECT SUM(FeeAMT) AS TotalFees 
FROM (
  SELECT FeeAMT FROM 131_OtherFees_and_Charges 
  UNION ALL 
  SELECT FeeAMT FROM 132_Loan_Origination_Fees
) AS FeesAndCharges;


INSERT INTO 131_OtherFees_and_Charges (FeeID, FeeAMT) 
VALUES (1, 100.00), (2, 50.00), (3, 25.00);

UPDATE 131_OtherFees_and_Charges 
SET FeeAMT = 75.00 
WHERE FeeID = 2;

INSERT INTO 132_Loan_Origination_Fees (FeeID, FeeAMT) 
VALUES (1, 200.00), (2, 150.00), (3, 75.00);

UPDATE 132_Loan_Origination_Fees 
SET FeeAMT = FeeAMT * 1.1 
WHERE FeeID = 1;



